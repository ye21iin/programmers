SELECT
    A.CATEGORY, A.MAX_PRICE, B.PRODUCT_NAME
FROM (
    SELECT CATEGORY, MAX(PRICE) AS MAX_PRICE
    FROM FOOD_PRODUCT 
    GROUP BY CATEGORY HAVING CATEGORY IN ('과자', '국', '김치', '식용유')
    ORDER BY MAX_PRICE DESC
) A
JOIN FOOD_PRODUCT B ON B.CATEGORY = A.CATEGORY AND B.PRICE = A.MAX_PRICE 
ORDER BY 2 DESC
